SUN_ICON = base64.decode("")
MOON_ICON = base64.decode("")
SYNTH_ICON = base64.decode("")
BATTERY_ICON = base64.decode("")
CURRENT_ICON = base64.decode("")
MONEYBAG_ICON = base64.decode("")

BAT_ICON_100 = base64.decode("")
BAT_ICON_75 = base64.decode("")
BAT_ICON_50 = base64.decode("")
BAT_ICON_25 = base64.decode("")
