    Monte_Carlo = base64.decode("")
    Sochi = base64.decode("")
    Jeddah = base64.decode("")
    Marina_Bay = base64.decode("")
    MontmelÃ³ = base64.decode("")
    Austin = base64.decode("")

    #Tack maps selection
    if F1_LOC == "Abu_Dhabi":
        MAP = Sakhir

    elif F1_LOC == "Spielberg":
        MAP = Spielberg

    elif F1_LOC == "Melbourne":
