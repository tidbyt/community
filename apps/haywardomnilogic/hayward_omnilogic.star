"""
Applet: Hayward Omnilogic
Summary: Display pool temperature
Description: Reads pool temperature from Hayward Omnilogic Controller and displays on the Tidbyt.
Author: kcharwood
"""

load("render.star", "render")
load("cache.star", "cache")
load("xpath.star", "xpath")
load("http.star", "http")
load("schema.star", "schema")
load("encoding/base64.star", "base64")
load("hash.star", "hash")

STATUS_KEY = "status"
STATUS_BAD_LOGIN = "bad_login"
STATUS_NO_TEMPERATURE = "no_temperature"
STATUS_SUCCESS = "success"
TEMPERATURE_KEY = "temperature"

POOL_IMAGE_BASE_64 = """

"""
POOL_IMAGE = base64.decode(POOL_IMAGE_BASE_64)

def main(config):
    api_token = config.get("api_token")
    if not api_token:
        return error_view("Missing API Token")
    system_id = config.get("system_id")
    if not system_id or len(system_id) != 6:
        return error_view("Missing System ID")
    pool_name = config.get("pool_name") or "Pool"
    result = get_pool_temperature(api_token = api_token, system_id = system_id)

    if result[STATUS_KEY] == STATUS_SUCCESS:
        temp_string = "{}".format(result[TEMPERATURE_KEY])
        return render.Root(
            child = render.Column(
                children = [
                    render.Stack(
                        children = [
                            render.Box(width = 64, height = 8, color = "#013"),
                            render.Row(
                                expanded = True,
                                main_align = "center",
                                children = [
                                    render.Text(color = "#FFF", content = pool_name),
                                ],
                            ),
                        ],
                    ),
                    render.Row(
                        expanded = True,
                        children = [
                            render.Image(src = POOL_IMAGE, width = 44, height = 24),
                            render.Row(
                                expanded = True,
                                main_align = "end",
                                cross_align = "end",
                                children = [
                                    render.Column(
                                        expanded = True,
                                        main_align = "end",
                                        cross_align = "end",
                                        children = [
                                            render.Text(color = "#FFF", content = temp_string),
                                        ],
                                    ),
                                ],
                            ),
                        ],
                    ),
                ],
            ),
        )
    elif result[STATUS_KEY] == STATUS_BAD_LOGIN:
        return error_view("Failed Login")
    else:
        return error_view("-")

def get_schema():
    return schema.Schema(
        version = "1",
        fields = [
            schema.Text(
                id = "api_token",
                name = "API Token",
                desc = "The API token for the Hayward API. Please see the README for instructions on how to generate a token with your username and password.",
                icon = "key",
            ),
            schema.Text(
                id = "system_id",
                name = "MSP ID",
                desc = "The MSP ID for your Hayward System. In the Hayward app, go to menu, and select About to see your MSP ID.",
                icon = "idBadge",
            ),
            schema.Text(
                id = "pool_name",
                name = "Pool Name",
                desc = "Name of the pool to show on the Tidbyt",
                icon = "personSwimming",
            ),
        ],
    )

def error_view(message):
    return render.Root(
        render.Row(
            expanded = True,
            main_align = "space_evenly",
            cross_align = "center",
            children = [
                render.Column(
                    expanded = True,
                    main_align = "center",
                    cross_align = "center",
                    children = [
                        render.Image(src = POOL_IMAGE, width = 20, height = 20),
                    ],
                ),
                render.Column(
                    expanded = True,
                    main_align = "center",
                    cross_align = "center",
                    children = [
                        render.WrappedText(font = "CG-pixel-3x5-mono", linespacing = 1, align = "center", content = message.upper()),
                    ],
                ),
            ],
        ),
    )

def get_pool_temperature(api_token, system_id):
    cache_key = "pool_response_{}".format(hash.sha1(system_id + api_token))
    response_body = cache.get(cache_key)
    if response_body == None:
        print(" Fetching new pool response")
        READINGS_URL = "https://www.haywardomnilogic.com/MobileInterface/MobileInterface.ashx"
        data = "<Request><Name>RequestTelemetryData</Name><Parameters><Parameter name=\"Token\" dataType=\"string\">{}</Parameter><Parameter name=\"MspSystemID\" dataType=\"int\">{}</Parameter></Parameters></Request>".format(api_token, system_id)
        rep = http.post(url = READINGS_URL, headers = {"Content-Type": "text/xml"}, body = data)
        print(rep.status_code)
        print(rep.body())
        response_body = rep.body()
        cache.set(cache_key, str(response_body), ttl_seconds = 600)
    else:
        print(" Using cached pool response")
    temps = []
    max_bodies_of_water = 10
    for index in range(1, max_bodies_of_water + 1):
        temp = xpath.loads(response_body).query("/STATUS/BodyOfWater[{}]/@waterTemp".format(index))
        if temp != None:
            temps.append(int(temp))
        else:
            break
    temps = [t for t in temps if t > 0]
    if temps:
        print("Pool temp is {}".format(max(temps)))
        return {STATUS_KEY: STATUS_SUCCESS, TEMPERATURE_KEY: max(temps)}
    else:
        return {STATUS_KEY: STATUS_NO_TEMPERATURE}
