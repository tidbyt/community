                                    ),
                                ),
                                render.Box(
                                    width = 24,
                                    height = 24,
                                    child = render.WrappedText(
                                        content = temp_content,
                                        color = temp_color,
                                        font = "5x8",
                                        align = "center",
                                    ),
                                ),
                                render.Row(
                                    children = [
                                        render.Box(
                                            height = 8,
                                            width = 8,
                                            child =
                                                render.Padding(
                                                    pad = (1, 1, 3, 0),
                                                    child = render.Image(
                                                        src = base64.decode(arrow_src),
                                                    ),
                                                ),
                                        ),
                                        render.Padding(
                                            pad = (1, 2, 0, 0),
                                            child = render.Text(
                                                content = "%d" % wind_speed,
                                                font = "tom-thumb",
                                            ),
                                        ),
                                    ],
                                ),
                            ],
                        ),
                    ],
                ),
                render.Column(
                    main_align = "end",
                    cross_align = "end",
                    expanded = True,
                    children = [
                        render.Row(
                            expanded = True,
                            main_align = "space_between",
                            cross_align = "end",
                            children = [
                                render.Image(
                                    src = img_render_sunrise,
                                ),
                                render.Text(
                                    content = "%s" % rise.in_location(loc["timezone"]).format("3:04"),
                                    font = "tom-thumb",
                                    color = "#F4D03F",
                                ),
                                render.Text(
                                    content = "%s" % set.in_location(loc["timezone"]).format("3:04"),
                                    font = "tom-thumb",
                                    color = "#BA4A00",
                                ),
                                render.Image(
                                    src = img_render_sunset,
                                ),
                            ],
                        ),
                        render.Row(
                            expanded = True,
                            main_align = "start",
                            children = [
                                render.Padding(
                                    pad = (6, 0, 6, 0),
                                    child = render.Box(
                                        width = box_width,
                                        height = 1,
                                        child = render.Image(
                                            src = img_render_suntracker,
                                        ),
                                    ),
                                ),
                            ],
                        ),
                    ],
                ),
